# Application
APP_NAME=unicorn-complete-example
APP_VERSION=1.0.0
APP_ENV=development

# HTTP Server
HTTP_HOST=0.0.0.0
HTTP_PORT=8080

# Database (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=unicorn_db
DB_USER=unicorn
DB_PASSWORD=unicorn_pass
DB_SSL_MODE=disable

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_GROUP_ID=unicorn-consumer-group

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
API_KEY=dev-api-key-12345

# Observability
ENABLE_METRICS=true
ENABLE_TRACING=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# Features
ENABLE_HTTP=true
ENABLE_BROKER=true
ENABLE_CRON=false
